@ECHO OFF

TITLE DEBUG SERVER

@REM GO FOR ROOT OF SERVICE
CD ../

@REM PATH TO JAVA
SET PATHJAVA=java

@REM FILE NAME
SET FILE=src/server.jar

@REM ARGS
SET ARGS=debug log

@REM RUN SERVER FOR DEBUG
%PATHJAVA% -jar %FILE% %ARGS%

IF ERRORLEVEL 1 GOTO error
GOTO end

:error
ECHO '
ECHO FILE "server.jar" NOT FOUND
ECHO '
PAUSE
EXIT

:end
PAUSE